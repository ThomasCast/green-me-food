<div class="container">
  <h1>Categories</h1>
    <% @categories.each do |category| %>
      <div class="card-category">

          <%= link_to category.name, category_path(category) %>
          <%= image_tag image_url(category.photo_url), width: 300 %>
          <% end %>
      </div>
  <hr>
</div>

